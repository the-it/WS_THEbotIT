# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
indent_style = space
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

[*.py]
indent_size = 4
max_line_length = 119
insert_final_newline = true

[*.json]
indent_size = 2

[Makefile]
indent_style = tab

[config.yml]
indent_size = 2

